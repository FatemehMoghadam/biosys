# biosys log
commands:
  01_create_dir:
    command: mkdir -p /var/log/biosys
  02_change_permissions:
    command: chmod -R g+s /var/log/biosys
  03_change_owner:
    command: chown -R wsgi:wsgi /var/log/biosys
option_settings:
  aws:elasticbeanstalk:application:environment:
    LOG_FOLDER: "/var/log/biosys"
